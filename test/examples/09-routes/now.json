{
  "version": 2,
  "builds": [{ "src": "index.php", "use": "vercel-php@latest" }],
  "routes": [{ "src": "/(.*)", "dest": "index.php" }],
  "probes": [
    { "path": "/any", "mustContain": "cow:RANDOMNESS_PLACEHOLDER:/any" },
    {
      "path": "/any?type=some",
      "mustContain": "cow:RANDOMNESS_PLACEHOLDER:/any?type=some"
    }
  ]
}
